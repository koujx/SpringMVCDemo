<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">
        
	<context:property-placeholder location="classpath:config-dubbo.properties"
		ignore-unresolvable="true" />
    <dubbo:consumer timeout="6000" />
    <dubbo:registry protocol="zookeeper" address="${zookeeper.address}"/>

    <!-- Ehr User相关服务 start -->
    <dubbo:reference id="ehrUserFacade" interface="com.lianjia.uc.api.user.EhrUserFacade" check="false"/>
    <dubbo:reference id="ehrOgrFacade" interface="com.lianjia.uc.api.org.EhrOrgFacade" check="false"/>
    <dubbo:reference id="ucAuthFacade" interface="com.lianjia.uc.api.auth.UcAuthFacade" check="false"/>
    <dubbo:reference id="housedelFacade" interface="com.lianjia.house.api.HousedelFacade" version="1.0.1" />

</beans>
